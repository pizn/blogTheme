---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>PIZn's GitHub Blog</title>
 <link href="http://pizn.github.com/blog/atom.xml" rel="self"/>
 <link href="http://pizn.github.com/blog/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://pizn.github.com/blog/</id>
 <author>
   <name>PIZn</name>
   <email>pizner@gmail.com</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://pizn.github.com/blog{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://pizn.github.com/blog{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>

